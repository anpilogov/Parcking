<Application x:Class="Plate.Trainer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Plate.Trainer"
             xmlns:properties="clr-namespace:Plate.Trainer.Properties"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
		<ResourceDictionary>
			<properties:Settings x:Key="Path" />
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary x:Name="IconsDictionary" 

				                    Source="pack://application:,,,/Plate.Trainer;component/Icons.xaml" />
			</ResourceDictionary.MergedDictionaries>
			</ResourceDictionary>
	</Application.Resources>
	
</Application>
